  <schema:Person rdf:about="http://philo.search/person/p63">
    <ns0:name>Rudolf Carnap</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1891</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1970</ns0:died>
    <ns0:sortby>Carnap, Rudolf</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t469"/>
    <ns0:said rdf:resource="http://philo.search/thought/t470"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t471">
        <ns0:value>The truths of logic are a by-product of the meaning of the language we use for reasoning and calculation.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t579"/>
