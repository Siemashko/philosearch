  <schema:Person rdf:about="http://philo.search/person/p90">
    <ns0:name>Thomas Kuhn</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1922</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1996</ns0:died>
    <ns0:sortby>Kuhn, Thomas</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t642"/>
    <ns0:said rdf:resource="http://philo.search/thought/t643"/>
    <ns0:said rdf:resource="http://philo.search/thought/t644"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t645">
        <ns0:value>The paradigms eventually use up their capacity to solve empirical puzzles, and are replaced by new paradigms.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t646">
        <ns0:value>A new paradigm is ‘incommensurable’ with the old one; there is no uncontested evidence and shared nomenclature to choose one.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t652"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t647"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t648">
        <ns0:value>There is no absolute distinction between the context of discovery and the context of justification.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t692"/>
      </rdf:Description>
