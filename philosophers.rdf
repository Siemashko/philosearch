  <schema:Person rdf:about="http://philo.search/person/p66">
    <ns0:name>Gilbert Ryle</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1900</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1976</ns0:died>
    <ns0:sortby>Ryle, Gilbert</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t490"/>
    <ns0:said rdf:resource="http://philo.search/thought/t491"/>
    <ns0:said rdf:resource="http://philo.search/thought/t492"/>
    <ns0:said rdf:resource="http://philo.search/thought/t493"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t494">
        <ns0:value>Having a mind is a matter of possessing abilities (knowing how to do things), dispositions, inclinations (emotions).</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t690"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t495">
        <ns0:value>When we perceive or have sensations, we are attending to features of the world or of our bodies, not to our experiences.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t808"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t496">
        <ns0:value>If our experiences have to be attended by us, that awareness experience also has to be attended – this sets off an absurd regress.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t808"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t497">
        <ns0:value>We can choose to attend to the experiences of perception/sensation, but it will be retrospective and fallible.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t498"/>
    <ns0:said rdf:resource="http://philo.search/thought/t499"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p67">
    <ns0:name>Frank Ramsey</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1903</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1930</ns0:died>
