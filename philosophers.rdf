  <schema:Person rdf:about="http://philo.search/person/p71">
    <ns0:name>Jean-Paul Sartre</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1905</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1980</ns0:died>
