  <schema:Person rdf:about="http://philo.search/person/p72">
    <ns0:name>Simone de Beauvoir</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1908</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1986</ns0:died>
