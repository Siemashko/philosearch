  <schema:Person rdf:about="http://philo.search/person/p47">
    <ns0:name>Karl Marx</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1818</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1883</ns0:died>
    <ns0:sortby>Marx, Karl</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t347"/>
    <ns0:said rdf:resource="http://philo.search/thought/t348"/>
    <ns0:said rdf:resource="http://philo.search/thought/t349"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t350">
        <ns0:value>The dialectic works with the development of the means of production and the conflicts between social classes.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t516"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t669"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t696"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t351">
        <ns0:value>Institutions, religions, philosophies, the arts, etc. are determined by the economic substructure.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t529"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t701"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t352"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t353">
        <ns0:value>The Industrialist Capitalist society is the last stage before the end of history, of the dialectic.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t489"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t354"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t355">
        <ns0:value>The workers will overthrow the capitalists and create a class-free society where the means of production belong to all.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t487"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t526"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t528"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t696"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p48">
    <ns0:name>Charles Sanders Peirce</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1839</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1914</ns0:died>
    <ns0:sortby>Peirce, Charles Sanders</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t356">
        <ns0:value>Knowledge is an activity: a process of doubt/need-to-know, evaluation, and understanding/explaining.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t361"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t421"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t424"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t426"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t483"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t357"/>
    <ns0:said rdf:resource="http://philo.search/thought/t358"/>
    <ns0:said rdf:resource="http://philo.search/thought/t359"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t360">
        <ns0:value>Truth is the final opinion on which inquiry is fated to converge.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t649"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t722"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t692"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

