      <rdf:Description rdf:about="http://philo.search/thought/t666">
        <ns0:value>With computers, knowledge has become information that can be stored in databases, and bought and sold.</ns0:value>
        <ns0:reference>The Philosophy Book, DK Pub., 2011</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t698"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t667">
        <ns0:value>Knowledge is becoming externalized; it is no longer something that helps towards the development of minds.</ns0:value>
        <ns0:reference>The Philosophy Book, DK Pub., 2011</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
