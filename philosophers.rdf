  <schema:Person rdf:about="http://philo.search/person/p73">
    <ns0:name>Albert Camus</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1913</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1960</ns0:died>
    <ns0:sortby>Camus, Albert</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t549"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t550">
        <ns0:value>The Absurd is not in man nor in the world, but in their presence together; it is the only bond uniting them.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/camus/, Author: David Simpson</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t551">
        <ns0:value>There is but one truly serious philosophical problem and that is suicide.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ethics</ns0:category>
