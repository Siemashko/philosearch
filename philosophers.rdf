  <schema:Person rdf:about="http://philo.search/person/p0">
    <ns0:name>Thales</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-624</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-580</ns0:died>
    <ns0:sortby>Thales</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t0">
        <ns0:value>Everything is water in one form or another.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t1"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t13"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p1">
    <ns0:name>Anaximander</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-610</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-546</ns0:died>
    <ns0:sortby>Anaximander</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t1"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p2">
    <ns0:name>Anaximenes</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-585</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-528</ns0:died>
    <ns0:sortby>Anaximenes</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t2"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p3">
    <ns0:name>Pythagoras</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-570</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-497</ns0:died>
    <ns0:sortby>Pythagoras</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t3">
        <ns0:value>We remember a lot of what we know from a previous life.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t30"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t4">
        <ns0:value>Mathematical order pervades the physical world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t28"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p4">
    <ns0:name>Xenophanes</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-570</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-475</ns0:died>
    <ns0:sortby>Xenophanes</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t5">
        <ns0:value>We may get nearer and nearer to the truth but our knowledge is our creation; it is in fact conjecture.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t26"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t68"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t359"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t6">
        <ns0:value>Each group shapes their gods in the likeness of their own.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t308"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t319"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p5">
    <ns0:name>Heraclitus</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-535</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-475</ns0:died>
    <ns0:sortby>Heraclitus</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t7">
        <ns0:value>Everything is a coming together of opposites.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t293"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t8">
        <ns0:value>There are no stable ‘things’; everything is flux, changing all the time.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t13"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t31"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t282"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t339"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t9">
        <ns0:value>A hidden connection is stronger than an obvious one.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t10">
        <ns0:value>It is not possible to step into the same river twice.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p6">
    <ns0:name>Parmenides</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-515</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-460</ns0:died>
    <ns0:sortby>Parmenides</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t11">
        <ns0:value>There can never have been nothing; everything must always have existed.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t289"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t12">
        <ns0:value>It is not possible for anything to pass into nothing; everything is eternal.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t13"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t14">
        <ns0:value>What appears as change is something that occurs within an enclosed and unchanging system.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p7">
    <ns0:name>Empedocles</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-490</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-430</ns0:died>
    <ns0:sortby>Empedocles</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t15"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p8">
    <ns0:name>Leucippus &amp; Democritus</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-500</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-401</ns0:died>
    <ns0:sortby>Leucippus &amp; Democritus</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t16">
        <ns0:value>Everything is made up of indivisible particles (atoms) that are too small to be seen.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t71"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t17">
        <ns0:value>All change in the universe consists of atoms altering formations or locations.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t54"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t72"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t18"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p9">
    <ns0:name>Zeno of Elea</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-490</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-430</ns0:died>
    <ns0:sortby>Zeno of Elea</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t19">
        <ns0:value>An impeccably logical argument can lead to a false conclusion.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t136"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t432"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p10">
    <ns0:name>Socrates</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-470</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-399</ns0:died>
    <ns0:sortby>Socrates</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t20">
        <ns0:value>Knowledge of the universe does not affect our behaviour.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t37"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t21">
        <ns0:value>We need to know how to conduct our lives and ourselves.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t37"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t65"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t22">
        <ns0:value>Things like ‘justice’ do exist; not materially, but in some sort of essence.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t27"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t34"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t112"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t23">
        <ns0:value>To a man who preserves his integrity no real, long-term harm can ever come.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t38"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t24">
        <ns0:value>We should pity the perpetrator of injustice, not the victim of injustice.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t39"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t25">
        <ns0:value>No one really knowingly does wrong.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t221"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t532"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t26"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p11">
    <ns0:name>Plato</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-427</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-347</ns0:died>
    <ns0:sortby>Plato</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t27"/>
    <ns0:said rdf:resource="http://philo.search/thought/t28"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t29">
        <ns0:value>The underlying perfect reality is accessible to the mind and the intellect.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t80"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t101"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t102"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t137"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t143"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t30"/>
    <ns0:said rdf:resource="http://philo.search/thought/t31"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t32">
        <ns0:value>Our bodies are ephemeral; our souls are our permanent Forms, timeless and indestructible.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t73"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t85"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t148"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t725"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t100"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t144"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t33">
        <ns0:value>Our aim in life should be to penetrate to the level of underlying reality.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t46"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t55"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t529"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t93"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t34"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t35">
        <ns0:value>Man is made up of three conflicting elements: passion (desire, appetite), intellect (reason), and will (spirit).</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t382"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t36">
        <ns0:value>Intellect should govern passions through the will.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>10</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t89"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t382"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t97"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t37"/>
    <ns0:said rdf:resource="http://philo.search/thought/t38"/>
    <ns0:said rdf:resource="http://philo.search/thought/t39"/>
    <ns0:said rdf:resource="http://philo.search/thought/t40"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t41">
        <ns0:value>A philosophically aware class should govern society through an intermediate police class for the happiness of the whole community.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>15</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t58"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t133"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t42">
        <ns0:value>Women share by nature in every way of life just as men do, but in all of them women are weaker than men.</ns0:value>
        <ns0:reference>Republic, Plato</ns0:reference>
        <ns0:order>16</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t62"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t333"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t43">
        <ns0:value>Girls should be brought up on equal terms with boys.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>17</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t272"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t333"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t44">
        <ns0:value>Art is doubly deceptive, creating illusory semblances of illusory semblances.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>18</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Aesthetics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t59"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t60"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t286"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t703"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p12">
    <ns0:name>Aristotle</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-384</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-322</ns0:died>
    <ns0:sortby>Aristotle</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t45"/>
    <ns0:said rdf:resource="http://philo.search/thought/t46"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t47">
        <ns0:value>A thing is whatever it is by virtue of its form.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t48">
        <ns0:value>The material cause of a thing is what it is made of.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t732"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t49">
        <ns0:value>The efficient cause of a thing is what does or makes it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t50">
        <ns0:value>The formal cause of a thing is the shape by which it’s identified.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t51">
        <ns0:value>The final cause of a thing is the ultimate reason for it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t52">
        <ns0:value>An object’s form, though not material, is inherent in this world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t53">
        <ns0:value>The real point of everything is what it does, what it is for.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t357"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t690"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t54"/>
    <ns0:said rdf:resource="http://philo.search/thought/t55"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t56">
        <ns0:value>To be happy, we must be compatible with living in a society; personal isolation is no good.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t70"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t77"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t57">
        <ns0:value>A virtue is the midway point between two extremes; our aim should be balance.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>13</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t382"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t466"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t58"/>
    <ns0:said rdf:resource="http://philo.search/thought/t59"/>
    <ns0:said rdf:resource="http://philo.search/thought/t60"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t61">
        <ns0:value>All men by nature desire to know.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>17</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t62"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t63">
        <ns0:value>Spoken words are the symbols of mental experience and written words are the symbols of spoken words.</ns0:value>
        <ns0:reference>On Interpretation, Aristotle</ns0:reference>
        <ns0:order>19</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t397"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t405"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t64">
        <ns0:value>All men have not the same writings or speech sounds, but the mental experiences which these directly symbolize are the same for all.</ns0:value>
        <ns0:reference>On Interpretation, Aristotle</ns0:reference>
        <ns0:order>20</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t379"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p13">
    <ns0:name>Diogenes</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-404</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-323</ns0:died>
    <ns0:sortby>Diogenes</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t65"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t66">
        <ns0:value>All social conventions (public/private, naked/clothed, etc.) are nonsense.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t67">
        <ns0:value>I am a citizen of the world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p14">
    <ns0:name>Pyrrho</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-360</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-270</ns0:died>
    <ns0:sortby>Pyrrho</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t68"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t69">
        <ns0:value>We cannot assume the truth of one explanation rather than any other.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t90"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t213"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t70"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p15">
    <ns0:name>Epicurus</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-341</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-270</ns0:died>
    <ns0:sortby>Epicurus</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t71"/>
    <ns0:said rdf:resource="http://philo.search/thought/t72"/>
    <ns0:said rdf:resource="http://philo.search/thought/t73"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t74">
        <ns0:value>Don’t fear death; so long as you exist, death is not, and when death is, you are not.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t129"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t75">
        <ns0:value>Gods are far away and have no desire to be involved with us; it’s as if they don’t exist.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t105"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t178"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t76"/>
    <ns0:said rdf:resource="http://philo.search/thought/t77"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t78">
        <ns0:value>We should enjoy pleasures in moderation, though no non-injurious activity needs to be forbidden.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t330"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t79">
        <ns0:value>The greatest pleasure is attainable through knowledge, friendship, and with freedom from fear.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Ethics</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p16">
    <ns0:name>Stoics (Zeno of Citium et al)</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-334</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-262</ns0:died>
    <ns0:sortby>Stoics (Zeno of Citium et al)</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t80"/>
    <ns0:said rdf:resource="http://philo.search/thought/t81"/>
    <ns0:said rdf:resource="http://philo.search/thought/t82"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t83">
        <ns0:value>The rationality within us and the Nature is what is meant by God.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Theology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t84">
        <ns0:value>God is not separate from the world – he is the mind, the self-awareness of the world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t146"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t85"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t86">
        <ns0:value>We should accept our mortality with dignity.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t152"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t87">
        <ns0:value>If our emotions rebel against our acceptance of mortality, they are in the wrong.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ethics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t88">
        <ns0:value>Emotions are cognitive judgements – forms of knowledge, true or false.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t89"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p17">
    <ns0:name>Timon of Phlius</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-320</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-230</ns0:died>
    <ns0:sortby>Timon of Phlius</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t90"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p18">
    <ns0:name>Plotinus</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">204</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">270</ns0:died>
    <ns0:sortby>Plotinus</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t91"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t92">
        <ns0:value>There are three ascending levels of being: soul (humans), intellect, and good.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t93"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p19">
    <ns0:name>Saint Augustine</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">354</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">430</ns0:died>
    <ns0:sortby>Augustine, Saint</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t94"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t95">
        <ns0:value>The flow of time does not exist independently of experience.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t248"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t419"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t96">
        <ns0:value>The present is the inescapable mode of all existence.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t97"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t98">
        <ns0:value>To doubt anything, I must exist; I cannot doubt that I exist – I know that with absolute certainty.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t141"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t99"/>
    <ns0:said rdf:resource="http://philo.search/thought/t100"/>
    <ns0:said rdf:resource="http://philo.search/thought/t101"/>
    <ns0:said rdf:resource="http://philo.search/thought/t102"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t103">
        <ns0:value>God did not create evil, because evil is not a thing, but a lack or deficiency of something.</ns0:value>
        <ns0:reference>The Philosophy Book, DK Pub., 2011</ns0:reference>
        <ns0:order>10</ns0:order>
        <ns0:category>Theology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t181"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t532"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t104">
        <ns0:value>To create rational beings, God left open the possibility that they would choose evil rather than good.</ns0:value>
        <ns0:reference>The Philosophy Book, DK Pub., 2011</ns0:reference>
        <ns0:order>11</ns0:order>
        <ns0:category>Theology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t187"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t268"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t105"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p20">
    <ns0:name>John Scotus Erigena</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">815</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">877</ns0:died>
    <ns0:sortby>Erigena, John Scotus</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t106">
        <ns0:value>Reason and revelation are both valid ways of arriving at the same truth.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t109"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t147"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t107">
        <ns0:value>Since God is unknowable, it is impossible for Him to know himself.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t114"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t162"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p21">
    <ns0:name>Saint Anselm</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1033</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1109</ns0:died>
    <ns0:sortby>Anselm, Saint</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t108">
        <ns0:value>Existence is greater than absence; so the most perfect being possible (God) must exist, by definition.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t112"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t207"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t250"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t142"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p22">
    <ns0:name>Thomas Aquinas</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1225</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1274</ns0:died>
    <ns0:sortby>Aquinas, Thomas</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t109"/>
    <ns0:said rdf:resource="http://philo.search/thought/t110"/>
    <ns0:said rdf:resource="http://philo.search/thought/t111"/>
    <ns0:said rdf:resource="http://philo.search/thought/t112"/>
    <ns0:said rdf:resource="http://philo.search/thought/t113"/>
    <ns0:said rdf:resource="http://philo.search/thought/t114"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t115">
        <ns0:value>The soul is known by its acts.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t805"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p23">
    <ns0:name>William of Ockham</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1287</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1347</ns0:died>
    <ns0:sortby>William of Ockham</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t116">
        <ns0:value>There is such a thing as necessity in logic but not in the natural order of things.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t585"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t766"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t117"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t118">
        <ns0:value>The more complicated explanation is more likely to have something wrong with it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t119">
        <ns0:value>We should always assume the minimum we need to assume in an explanation; entities should not be posited unnecessarily.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t124"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t148"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t479"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t493"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t622"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t800"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t814"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t142"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t144"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t185"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t773"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p24">
    <ns0:name>Francis Bacon</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1561</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1626</ns0:died>
    <ns0:sortby>Bacon, Francis</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t120"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t121">
        <ns0:value>Negative instances are as important as positive ones in guiding us to the right conclusions.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t481"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t122"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t123">
        <ns0:value>Words are but the images of matter, to fall in love with them is to fall in love with a picture.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t446"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p25">
    <ns0:name>Thomas Hobbes</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1588</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1679</ns0:died>
    <ns0:sortby>Hobbes, Thomas</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t124"/>
    <ns0:said rdf:resource="http://philo.search/thought/t125"/>
    <ns0:said rdf:resource="http://philo.search/thought/t126"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t127">
        <ns0:value>Psychological motivations are sorts of pushes: appetites and aversions.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t150"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t380"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t384"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t128">
        <ns0:value>Appetites (liking, love, etc.) are inherently unsatisfiable; they cannot cease until death.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t314"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t129"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t130">
        <ns0:value>At the bottom, the fear of death causes people to form societies.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t168"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t131">
        <ns0:value>In the state of nature there is no order or justice, violence and cunning rules; life is solitary, poor, nasty, brutish and short.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t167"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t202"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t221"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t132">
        <ns0:value>The alliances should be such that it is not in anyone’s interest to break laws.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t230"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t133"/>
    <ns0:said rdf:resource="http://philo.search/thought/t134"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t135">
        <ns0:value>Any tyranny is better than social chaos.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t170"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p26">
    <ns0:name>René Descartes</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1596</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1650</ns0:died>
    <ns0:sortby>Descartes, René</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t136"/>
    <ns0:said rdf:resource="http://philo.search/thought/t137"/>
    <ns0:said rdf:resource="http://philo.search/thought/t138"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t139">
        <ns0:value>I cannot be absolutely sure at any moment that I am not being deceived by an evil demon controlling my senses.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t687"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t140">
        <ns0:value>The only thing I can be sure of is that I am having the experiences that I am having.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t156"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t406"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t561"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t597"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t141"/>
    <ns0:said rdf:resource="http://philo.search/thought/t142"/>
    <ns0:said rdf:resource="http://philo.search/thought/t143"/>
    <ns0:said rdf:resource="http://philo.search/thought/t144"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p27">
    <ns0:name>Benedict Spinoza</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1632</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1677</ns0:died>
    <ns0:sortby>Spinoza, Benedict</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t145"/>
    <ns0:said rdf:resource="http://philo.search/thought/t146"/>
    <ns0:said rdf:resource="http://philo.search/thought/t147"/>
    <ns0:said rdf:resource="http://philo.search/thought/t148"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t149">
        <ns0:value>All things are alive and have minds, in different degrees.</ns0:value>
        <ns0:reference>Ethics, Benedict Spinoza, 1677</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t176"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t190"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t266"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t836"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t281"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t150"/>
    <ns0:said rdf:resource="http://philo.search/thought/t151"/>
    <ns0:said rdf:resource="http://philo.search/thought/t152"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t153">
        <ns0:value>Freedom of speech is necessary to secure the public order.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t169"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t330"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t154"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p28">
    <ns0:name>John Locke</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1632</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1704</ns0:died>
    <ns0:sortby>Locke, John</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t155"/>
    <ns0:said rdf:resource="http://philo.search/thought/t156"/>
    <ns0:said rdf:resource="http://philo.search/thought/t157"/>
    <ns0:said rdf:resource="http://philo.search/thought/t158"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t159">
        <ns0:value>Science can deal with primary (objective, measurable) qualities; length, weight, velocity, etc.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t252"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t160">
        <ns0:value>Secondary (subjective) qualities are out of the reach of science: taste, smell, colour, etc.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t188"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t335"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t829"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t830"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t812"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t161">
        <ns0:value>We can only know the properties of a thing, not the material thing in itself.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t196"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t771"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t246"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t162"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t163">
        <ns0:value>Possible knowledge consists entirely of transactions between the mysterious subject and object.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t197"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t164"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t165">
        <ns0:value>We must always be willing to change our beliefs in the light of changing evidence.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>11</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t217"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t166">
        <ns0:value>It is mistaken and morally wrong for political and religious authorities to impose beliefs.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t487"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t167"/>
    <ns0:said rdf:resource="http://philo.search/thought/t168"/>
    <ns0:said rdf:resource="http://philo.search/thought/t169"/>
    <ns0:said rdf:resource="http://philo.search/thought/t170"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t171">
        <ns0:value>If I work to produce something, then I have a right to the fruits of my labour.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>17</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t760"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t172">
        <ns0:value>I can dispose of my property right as I wish; give it, sell it, etc., independently of government.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>18</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t764"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p29">
    <ns0:name>Anne Conway</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1631</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1679</ns0:died>
    <ns0:sortby>Conway, Anne</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t173"/>
    <ns0:said rdf:resource="http://philo.search/thought/t174"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t175">
        <ns0:value>Created substance, like God, consists of spirit, but, unlike God, is constituted of particles (‘monads’).</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/conway/, Author: Sarah Hutton</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t189"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t190"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t192"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t176"/>
    <ns0:said rdf:resource="http://philo.search/thought/t177"/>
    <ns0:said rdf:resource="http://philo.search/thought/t178"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t179">
        <ns0:value>All things are capable of becoming more spirit-like – more refined qua spiritual substance.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/conway/, Author: Sarah Hutton</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t180">
        <ns0:value>All things are capable of increased goodness.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/conway/, Author: Sarah Hutton</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t202"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t181"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p30">
    <ns0:name>Gottfried Wilhelm Leibniz</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1646</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1716</ns0:died>
    <ns0:sortby>Leibniz, Gottfried Wilhelm</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t182">
        <ns0:value>There are two kinds of truths: (analytic) truths of reasoning and (synthetic) truths of fact.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t249"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t579"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t183">
        <ns0:value>The sufficient reason for a truth of reasoning is to be found by reasoning, without reference to external reality.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t215"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t472"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t578"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t184">
        <ns0:value>The sufficient reason for a truth of fact is to be found by looking at external facts.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t185"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t186">
        <ns0:value>God, as a perfect being, created the best possible world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t187"/>
    <ns0:said rdf:resource="http://philo.search/thought/t188"/>
    <ns0:said rdf:resource="http://philo.search/thought/t189"/>
    <ns0:said rdf:resource="http://philo.search/thought/t190"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t191">
        <ns0:value>Human minds are monads.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>10</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t192"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t193">
        <ns0:value>Nature makes no leaps. (The law of continuity)</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p31">
    <ns0:name>George Berkeley</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1685</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1753</ns0:died>
    <ns0:sortby>Berkeley, George</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t194"/>
    <ns0:said rdf:resource="http://philo.search/thought/t195"/>
    <ns0:said rdf:resource="http://philo.search/thought/t196"/>
    <ns0:said rdf:resource="http://philo.search/thought/t197"/>
    <ns0:said rdf:resource="http://philo.search/thought/t198"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p32">
    <ns0:name>Francis Hutcheson</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1694</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1746</ns0:died>
    <ns0:sortby>Hutcheson, Francis</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t199">
        <ns0:value>Besides his five external senses, man has internal senses including a sense of beauty, of morality, of honour, and of the ridiculous.</ns0:value>
        <ns0:reference>An Inquiry into the Original of our Ideas of Beauty and Virtue, Francis Hutcheson, 1725</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t200">
        <ns0:value>Moral sense is our most important sense.</ns0:value>
        <ns0:reference>An Inquiry into the Original of our Ideas of Beauty and Virtue, Francis Hutcheson, 1725</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t278"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t343"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t201">
        <ns0:value>Moral sense is implanted in man, instinctively approving virtuous actions and disapproving the vicious ones.</ns0:value>
        <ns0:reference>Encyclopaedia Britannica, britannica.com/biography/Francis-Hutcheson</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t219"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t441"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t307"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t202"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t203">
        <ns0:value>Wisdom denotes the pursuing of the best ends by the best means.</ns0:value>
        <ns0:reference>Encyclopaedia Britannica, britannica.com/biography/Francis-Hutcheson</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ethics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t204"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p33">
    <ns0:name>David Hume</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1711</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1776</ns0:died>
    <ns0:sortby>Hume, David</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t205"/>
    <ns0:said rdf:resource="http://philo.search/thought/t206"/>
    <ns0:said rdf:resource="http://philo.search/thought/t207"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t208">
        <ns0:value>There is no observational evidence for the existence of God; only some inferential, indirect arguments.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t251"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t209">
        <ns0:value>The Argument from Design is a far cry from proof of a personal God, Christian or Jew.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t308"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t210">
        <ns0:value>Causal connection cannot be observed; correlation is not causation.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t211">
        <ns0:value>However many Xs I observe having the property y, that is no proof that the next X I see will be y.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t275"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t483"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t212">
        <ns0:value>We should not define a cause-effect relationship as a matter of conceptual necessity.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t610"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t213"/>
    <ns0:said rdf:resource="http://philo.search/thought/t214"/>
    <ns0:said rdf:resource="http://philo.search/thought/t215"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t216">
        <ns0:value>With this level of uncertainty, Theories of Everything (in philosophy, science, etc.) are ridiculous.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t487"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t568"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t669"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t217"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t218">
        <ns0:value>Deductions of ‘ought’ statements from ‘is’ statements need careful observation and explanation.</ns0:value>
        <ns0:reference>A Treatise of Human Nature, David Hume, 1739</ns0:reference>
        <ns0:order>14</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t630"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t219"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t220">
        <ns0:value>Beauty in things exists in the mind which contemplates them.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>16</ns0:order>
        <ns0:category>Aesthetics</ns0:category>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p34">
    <ns0:name>Jean-Jacques Rousseau</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1712</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1778</ns0:died>
    <ns0:sortby>Rousseau, Jean-Jacques</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t221"/>
    <ns0:said rdf:resource="http://philo.search/thought/t222"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t223">
        <ns0:value>We cannot be uncivilized, so we should change the civilization to make instincts and feelings free.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t387"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t224">
        <ns0:value>Education should not discipline a child’s natural tendencies; it should encourage them.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t225">
        <ns0:value>The tools of education should not be books but practice: experience of people and things.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t426"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t226">
        <ns0:value>Education should take place in family, not school; and through love, not rules and punishment.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t227"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t228">
        <ns0:value>Human society is a collective being with a will of its own.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t235"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t229"/>
    <ns0:said rdf:resource="http://philo.search/thought/t230"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t231">
        <ns0:value>The rulers can give the task of execution to whoever they choose; a monarch, politicians, etc.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>11</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t232">
        <ns0:value>A population at large might constitute an ill-informed, short-sighted legislative body.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t233">
        <ns0:value>‘Legislators’, charismatic leaders who understand the general will, can draft legislation and persuade people.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>13</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p35">
    <ns0:name>Edmund Burke</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1729</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1797</ns0:died>
    <ns0:sortby>Burke, Edmund</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t234">
        <ns0:value>Society is so big and complex that a single mind cannot possibly contain and understand it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t267"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t294"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t235"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t236">
        <ns0:value>The only realistic mode of political change is organic, not revolutionary.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t237">
        <ns0:value>Each generation inherits a treasure from the past which it is its duty to pass on.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t797"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t238">
        <ns0:value>Humans are imperfect creatures, therefore the idea of a perfect society is fantasy.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t296"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t489"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t239"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t240">
        <ns0:value>The public prospers more under practical-minded men than under clever theoreticians.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t241">
        <ns0:value>Great art seeks the infinite, which cannot be clear; it has the emotional pull of the unknown, the sublime.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Aesthetics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t674"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p36">
    <ns0:name>Immanuel Kant</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1724</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1804</ns0:died>
    <ns0:sortby>Kant, Immanuel</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t242"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t243">
        <ns0:value>The contents of consciousness, produced by our bodily apparatus, are just limited representations.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t311"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t396"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t244">
        <ns0:value>Our senses give us accurate information, but they do so as gauges, not pictures.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t591"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t811"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t245">
        <ns0:value>The mode of existence of things (noumena) has nothing to do with our impressions (phenomena) of them.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t301"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t393"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t412"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t480"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t246"/>
    <ns0:said rdf:resource="http://philo.search/thought/t247"/>
    <ns0:said rdf:resource="http://philo.search/thought/t248"/>
    <ns0:said rdf:resource="http://philo.search/thought/t249"/>
    <ns0:said rdf:resource="http://philo.search/thought/t250"/>
    <ns0:said rdf:resource="http://philo.search/thought/t251"/>
    <ns0:said rdf:resource="http://philo.search/thought/t252"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t253">
        <ns0:value>If we are objects moving in space and time with scientific laws, we cannot have free will.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>12</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t707"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t813"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t254"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t255">
        <ns0:value>We believe that there is a non-empirical realm in which decisions are made, that affects our bodies.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>14</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t302"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t303"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t256"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t257">
        <ns0:value>Because morality concerns the choice of action, moral judgements take the form of imperatives.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>16</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t630"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t758"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t258"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t259">
        <ns0:value>You should act only according to maxims which you can will also to be universal laws.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>18</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t533"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t631"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t638"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t639"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t754"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p37">
    <ns0:name>Jeremy Bentham</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1748</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1832</ns0:died>
    <ns0:sortby>Bentham, Jeremy</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t260">
        <ns0:value>The rightness/wrongness of an action is determined by its consequences.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t609"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t261">
        <ns0:value>Good consequences of actions are those that give pleasure to someone (and bad – pain).</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ethics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t262"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t263">
        <ns0:value>Everybody is to count for one; and nobody for more than one.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t639"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t695"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t264"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t265">
        <ns0:value>All punishment is mischief: all punishment in itself is evil.</ns0:value>
        <ns0:reference>Principles of Morals and Legislation, Jeremy Bentham, 1789</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t266"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p38">
    <ns0:name>Mary Wollstonecraft</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1759</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1797</ns0:died>
    <ns0:sortby>Wollstonecraft, Mary</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t267"/>
    <ns0:said rdf:resource="http://philo.search/thought/t268"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t269">
        <ns0:value>Rights entail duties; none could be expected to perform duties whose natural rights are not respected.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/wollstonecraft/, Author: Sylvana Tomaselli</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t270">
        <ns0:value>For a truly moral society, a complete change in the nature of the relationship between men and women is required.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/wollstonecraft/, Author: Sylvana Tomaselli</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t271">
        <ns0:value>Women are ill-prepared for their duties as social beings and imprisoned in a web of false expectations.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/wollstonecraft/, Author: Sylvana Tomaselli</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t545"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t272"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t273">
        <ns0:value>Women should be able to support themselves and their children, and never have to marry or remarry out of financial necessity.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/wollstonecraft/, Author: Sylvana Tomaselli</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t274">
        <ns0:value>Women should be granted civil and political rights; they should elect representatives of their own.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/wollstonecraft/, Author: Sylvana Tomaselli</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t333"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t548"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p39">
    <ns0:name>Johann Gottlieb Fichte</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1762</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1814</ns0:died>
    <ns0:sortby>Fichte, Johann Gottlieb</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t275"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t276">
        <ns0:value>Empirical observations can be deduced from scientific laws.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t485"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t277"/>
    <ns0:said rdf:resource="http://philo.search/thought/t278"/>
    <ns0:said rdf:resource="http://philo.search/thought/t279"/>
    <ns0:said rdf:resource="http://philo.search/thought/t280"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p40">
    <ns0:name>Friedrich Schelling</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1775</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1854</ns0:died>
    <ns0:sortby>Schelling, Friedrich</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t281"/>
    <ns0:said rdf:resource="http://philo.search/thought/t282"/>
    <ns0:said rdf:resource="http://philo.search/thought/t283"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t284">
        <ns0:value>Man is part of Nature, not separate from or above it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t285">
        <ns0:value>Man’s creativity is different from the rest of Nature’s creativity; it is self-aware.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t802"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t286"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t287">
        <ns0:value>The end-point of the process of Nature is self-awareness.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t292"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t288">
        <ns0:value>Creative art is the summit of all existence, the raison d’être of reality.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Aesthetics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t289"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p41">
    <ns0:name>Georg Wilhelm Friedrich Hegel</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1770</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1831</ns0:died>
    <ns0:sortby>Hegel, Georg Wilhelm Friedrich</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t290"/>
    <ns0:said rdf:resource="http://philo.search/thought/t291"/>
    <ns0:said rdf:resource="http://philo.search/thought/t292"/>
    <ns0:said rdf:resource="http://philo.search/thought/t293"/>
    <ns0:said rdf:resource="http://philo.search/thought/t294"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t295">
        <ns0:value>When Geist will reach self-awareness, everything will be harmonious and change will stop (the Absolute).</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t296"/>
    <ns0:said rdf:resource="http://philo.search/thought/t297"/>
    <ns0:said rdf:resource="http://philo.search/thought/t298"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t299">
        <ns0:value>One’s self-consciousness is dependent on one’s recognition of those others as similarly recognizing oneself as a self-conscious subject.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/hegel/, Author: Paul Redding</ns0:reference>
        <ns0:order>10</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t410"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t503"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t542"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t597"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p42">
    <ns0:name>Arthur Schopenhauer</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1788</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1860</ns0:died>
    <ns0:sortby>Schopenhauer, Arthur</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t300">
        <ns0:value>Without time and space there can be no differentiation.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t301"/>
    <ns0:said rdf:resource="http://philo.search/thought/t302"/>
    <ns0:said rdf:resource="http://philo.search/thought/t303"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t304">
        <ns0:value>The phenomena in the universe requires a vast impersonal, unconscious energy without purpose.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t413"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t305">
        <ns0:value>The energy, the force, the impersonal ‘will’ that we see in the physical events is what noumenal is.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t306"/>
    <ns0:said rdf:resource="http://philo.search/thought/t307"/>
    <ns0:said rdf:resource="http://philo.search/thought/t308"/>
    <ns0:said rdf:resource="http://philo.search/thought/t309"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t310">
        <ns0:value>The empirical world is without meaning and purpose.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>11</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t364"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t463"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t549"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t311"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t312">
        <ns0:value>We should not be taken by the empirical world; we should renounce it.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>13</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t365"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t313">
        <ns0:value>The end stage of philosophical understanding is the turning away of the human will from the world.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>14</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t314"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t315">
        <ns0:value>We can only escape existential suffering through aesthetic contemplation or asceticism.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>16</ns0:order>
        <ns0:category>Aesthetics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t316"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t317">
        <ns0:value>Music transcends the others arts in metaphysical significance.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>18</ns0:order>
        <ns0:category>Aesthetics</ns0:category>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p43">
    <ns0:name>Ludwig Feuerbach</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1804</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1872</ns0:died>
    <ns0:sortby>Feuerbach, Ludwig</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t318">
        <ns0:value>God is an imaginary representation of the human species-essence; theology is anthropology.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/ludwig-feuerbach/, Author: Todd Gooch</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Theology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t352"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t319"/>
    <ns0:said rdf:resource="http://philo.search/thought/t320"/>
    <ns0:said rdf:resource="http://philo.search/thought/t321"/>
    <ns0:said rdf:resource="http://philo.search/thought/t322"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t323">
        <ns0:value>We need to renounce the theological essence of religion to affirm its true anthropological essence: the divinity of man.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/ludwig-feuerbach/, Author: Todd Gooch</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Theology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t324"/>
    <ns0:said rdf:resource="http://philo.search/thought/t325"/>
    <ns0:said rdf:resource="http://philo.search/thought/t326"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p44">
    <ns0:name>John Stuart Mill</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1806</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1873</ns0:died>
    <ns0:sortby>Mill, John Stuart</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t327"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t328">
        <ns0:value>The concept of freedom isn’t meaningful in the state of nature.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t526"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t329">
        <ns0:value>Freedom can flourish only where there are political and legal institutions protecting individual interests and public good.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t530"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t330"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t331">
        <ns0:value>There should exist the fullest liberty of professing and discussing any doctrine, however immoral it may be considered.</ns0:value>
        <ns0:reference>On Liberty, John Stuart Mill,  John W.Parker &amp; Son., 1859</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t653"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t332">
        <ns0:value>Over his own body and mind, the individual is sovereign.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t760"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t333"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t334">
        <ns0:value>The semantic value of a name is its referent.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/names/, Author: Sam Cumming</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t434"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t593"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t335"/>
