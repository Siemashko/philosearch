    <ns0:order>2</ns0:order>
    <ns0:category>Ontology</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t732">
    <ns0:value>Mental states are intrinsic and irreducible features of certain very complex kinds of biological system.</ns0:value>
    <ns0:reference>Encyclopædia Britannica, Article: John Searle, Contributor: Nicholas Fotion</ns0:reference>
