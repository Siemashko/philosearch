    <ns0:order>4</ns0:order>
    <ns0:category>Ontology</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t748">
    <ns0:value>The complexity of our minds is irrelevant to the question of whether they’re the products of natural selection.</ns0:value>
