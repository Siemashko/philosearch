    <ns0:order>1</ns0:order>
    <ns0:category>Ontology</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t18">
    <ns0:value>The universe is not a continuum, but consists of separate entities.</ns0:value>
    <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
    <ns0:order>3</ns0:order>
    <ns0:category>Ontology</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t26">
    <ns0:value>There can be no cut and dried answers; answers themselves are open to question.</ns0:value>
    <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
    <ns0:order>7</ns0:order>
    <ns0:category>Epsitemology</ns0:category>
