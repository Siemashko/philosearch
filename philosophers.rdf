  <schema:Person rdf:about="http://philo.search/person/p70">
    <ns0:name>Hannah Arendt</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1906</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1975</ns0:died>
    <ns0:sortby>Arendt, Hannah</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t524">
        <ns0:value>The appeal of totalitarian ideologies rests upon the devastation of ordered and stable contexts in which people once lived.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/arendt/, Author: Majid Yar</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t525">
        <ns0:value>There are three types of human activities: labor, work, and action (in ascending hierarchy of importance).</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/arendt/, Author: Majid Yar</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t526"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t527">
        <ns0:value>Work is the fabrication of quasi-permanent physical and cultural artifacts: a non-animal, public activity with some degree of freedom.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/arendt/, Author: Majid Yar</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

