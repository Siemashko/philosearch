  <schema:Person rdf:about="http://philo.search/person/p45">
    <ns0:name>Charles Darwin</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1809</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1882</ns0:died>
    <ns0:sortby>Darwin, Charles</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t336"/>
    <ns0:said rdf:resource="http://philo.search/thought/t337"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t338">
        <ns0:value>It is hardly possible to define clearly what is meant by the organisation of a living thing being higher or lower.</ns0:value>
        <ns0:reference>The Origin of Species, Charles Darwin, John Murray, 1859</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t650"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t339"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t340">
        <ns0:value>The gradual formation of different languages is similar to that of species; the survival of favored words is natural selection.</ns0:value>
        <ns0:reference>The Descent of Man, Charles Darwin, John Murray, 1871</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t402"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t797"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t341">
        <ns0:value>The study of nature and man will become far more interesting when we regard every production of nature as one which has had a history.</ns0:value>
        <ns0:reference>The Origin of Species, Charles Darwin, John Murray, 1859</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t347"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t393"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t411"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t800"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t748"/>
      </rdf:Description>
    </ns0:said>

  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p46">
    <ns0:name>Søren Kierkegaard</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1813</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1855</ns0:died>
