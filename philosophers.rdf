    <ns0:agreedWith rdf:resource="http://philo.search/thought/t146"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t15">
    <ns0:value>Everything is made up of four elements that are perennial: earth, water, air, and fire.</ns0:value>
