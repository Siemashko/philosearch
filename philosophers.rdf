    <ns0:order>6</ns0:order>
    <ns0:category>Epsitemology</ns0:category>
    <ns0:category>Ontology</ns0:category>
    <ns0:category>Political Philosophy</ns0:category>
    <ns0:agreedWith rdf:resource="http://philo.search/thought/t755"/>
    <ns0:disagreedWith rdf:resource="http://philo.search/thought/t825"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t729">
    <ns0:value>Both sexes have to reconfigure their subjectivity so that they both understand themselves as belonging equally to nature and culture.</ns0:value>
    <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/irigaray/, Author: Sarah K. Donovan</ns0:reference>
