    <ns0:said rdf:resource="http://philo.search/thought/t480"/>
    <ns0:said rdf:resource="http://philo.search/thought/t481"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t482">
        <ns0:value>Theories are ‘corroborated’ when they survive several refutations; the better corroborated it is, the more it approximates to the truth.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t649"/>
