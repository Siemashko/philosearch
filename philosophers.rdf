    <ns0:said rdf:resource="http://philo.search/thought/t473"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p64">
    <ns0:name>Carl Hempel</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1905</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1997</ns0:died>
    <ns0:sortby>Hempel, Carl</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t474">
        <ns0:value>The main aim of science is explanation.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t475">
        <ns0:value>A good scientific explanation is one that would have enabled the phenomenon to be predicted.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t476">
        <ns0:value>The logic of science is a combination of the deductive refutation and the inductive confirmation of theoretical predictions by observation.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t481"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t642"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t501"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t477"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t478">
        <ns0:value>Predictions of a theory should be couched in an ‘observation-language’ whose meaning is independent of the theory.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t485"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t479"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p65">
    <ns0:name>Karl Popper</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1902</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1994</ns0:died>
