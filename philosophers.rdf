  <schema:Person rdf:about="http://philo.search/person/p82">
    <ns0:name>Elizabeth Anscombe</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1919</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2001</ns0:died>
    <ns0:sortby>Anscombe, Elizabeth</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t602">
        <ns0:value>Actions are identified and understood by reference to the intentions with which they were performed.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t610"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t625"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t789"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t780"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t603"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t604">
        <ns0:value>Unintentional causes of action (‘mental causes’) are not reasons for action.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t605"/>
    <ns0:said rdf:resource="http://philo.search/thought/t606"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t607">
        <ns0:value>A secular morality requires a new philosophy of mind which will break with the old ways of thinking about the roles of beliefs and desires in explaining action.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t608">
        <ns0:value>The practice of the virtues should connect with a potentiality for ‘human flourishing’.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ethics</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t609"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p83">
    <ns0:name>Peter Winch</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1926</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1997</ns0:died>
    <ns0:sortby>Winch, Peter</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t610"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t611">
        <ns0:value>The relationship between an action and a reason does not have to be the same every time and for every agent, unlike a causal relationship.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Ethics</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t623"/>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t791"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t612">
        <ns0:value>The study of society is about the logical relationships between the beliefs and intentions, and the rules of social institutions.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t613"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p84">
    <ns0:name>Donald Davidson</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1917</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2003</ns0:died>
    <ns0:sortby>Davidson, Donald</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t614"/>
    <ns0:said rdf:resource="http://philo.search/thought/t615"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t616">
        <ns0:value>Language isn’t a network of social conventions; there are just the general grammatical capacities and individual communicative strategies.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>3</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t681"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t617"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t618">
        <ns0:value>One cannot be a thinker at all unless one has beliefs.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t619"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t620">
        <ns0:value>It is only through seeing others’ errors that one can understand the possibility of being in error oneself.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t621">
        <ns0:value>A creature cannot have thoughts unless it is an interpreter of the speech of another; “no thought without talk.”</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>8</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
        <ns0:agreedWith rdf:resource="http://philo.search/thought/t811"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t622"/>
    <ns0:said rdf:resource="http://philo.search/thought/t623"/>
    <ns0:said rdf:resource="http://philo.search/thought/t624"/>
