    <ns0:said rdf:resource="http://philo.search/thought/t344"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t345">
        <ns0:value>The relationship of the individual soul to God is what matters most.</ns0:value>
        <ns0:reference>The Story of Philosophy, Bryan Magee, DK Pub., 1998</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Theology</ns0:category>
