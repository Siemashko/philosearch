    <ns0:category>Epsitemology</ns0:category>
    <ns0:category>Ontology</ns0:category>
    <ns0:category>Political Philosophy</ns0:category>
    <ns0:agreedWith rdf:resource="http://philo.search/thought/t826"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t728">
    <ns0:value>Inclusion of women in the current form of subjectivity is not the solution; there should be more than one subject position in language.</ns0:value>
