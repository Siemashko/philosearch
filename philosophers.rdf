  <schema:Person rdf:about="http://philo.search/person/p102">
    <ns0:name>John Searle</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1932</ns0:born>
    <ns0:sortby>Searle, John</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t730"/>
    <ns0:said rdf:resource="http://philo.search/thought/t731"/>
    <ns0:said rdf:resource="http://philo.search/thought/t732"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t733">
        <ns0:value>Unless a mental state has the potential to be conscious, it does not qualify as genuinely mental.</ns0:value>
        <ns0:reference>Stanford Encyclopedia of Philosophy, plato.stanford.edu/entries/intentionality/, Author: Pierre Jacob</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t734">
        <ns0:value>Real understanding cannot be a matter of mere symbol manipulation; computers cannot be conscious.</ns0:value>
        <ns0:reference>Encyclopædia Britannica, Article: John Searle, Contributor: Nicholas Fotion</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t803"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t735"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t736">
        <ns0:value>Language and other artifacts have intentionality derived from the ‘original/intrinsic intentionality’ of our conscious mental states.</ns0:value>
        <ns0:reference>Mind, Language and Society, John Searle, Basic Books, 1999</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ontology</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t804"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t737"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t738">
        <ns0:value>We-intentions (collective intentions) are held by individuals but they are not reducible to I-intentions.</ns0:value>
        <ns0:reference>Encyclopædia Britannica, Article: John Searle, Contributor: Nicholas Fotion</ns0:reference>
        <ns0:order>9</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t739">
        <ns0:value>Objective social reality (games, money, language, etc.) is created by means of we-intentions.</ns0:value>
        <ns0:reference>Encyclopædia Britannica, Article: John Searle, Contributor: Nicholas Fotion</ns0:reference>
        <ns0:order>10</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t740"/>
    <ns0:said rdf:resource="http://philo.search/thought/t741"/>
  </schema:Person>

  <schema:Person rdf:about="http://philo.search/person/p103">
    <ns0:name>Jerry Fodor</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1935</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2017</ns0:died>
    <ns0:sortby>Fodor, Jerry</ns0:sortby>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t742">
        <ns0:value>Every event that falls under a special science predicate also falls under a physical predicate, but not vice versa.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/fodor/, Author: Bradley Rives</ns0:reference>
        <ns0:order>1</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
