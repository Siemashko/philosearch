  <schema:Person rdf:about="http://philo.search/person/p92">
    <ns0:name>Michel Foucault</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1926</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1984</ns0:died>
