    <ns0:category>Epsitemology</ns0:category>
    <ns0:category>Ontology</ns0:category>
    <ns0:category>Political Philosophy</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t730">
    <ns0:value>Five types of speech acts: assertives (e.g. statements), directives (e.g. orders), commissives (e.g. promises), expressives (e.g. thanks), and declarations (e.g. hirings).</ns0:value>
