  <schema:Person rdf:about="http://philo.search/person/p89">
    <ns0:name>John Rawls</ns0:name>
    <ns0:born rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1921</ns0:born>
    <ns0:died rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2002</ns0:died>
    <ns0:sortby>Rawls, John</ns0:sortby>
    <ns0:said rdf:resource="http://philo.search/thought/t635"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t636">
        <ns0:value>The construction of justice is guided by our recognition of a range of primary, fundamental goods.</ns0:value>
        <ns0:reference>Contemporary Philosophy, Thomas Baldwin, Oxford University Press, 2001</ns0:reference>
        <ns0:order>2</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t789"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t637"/>
    <ns0:said rdf:resource="http://philo.search/thought/t638"/>
    <ns0:said rdf:resource="http://philo.search/thought/t639"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t640">
        <ns0:value>Social and economic inequalities are to be arranged so that they are to the greatest benefit of the least advantaged.</ns0:value>
        <ns0:reference>A Theory of Justice, John Rawls, Harvard University Press, 1971</ns0:reference>
        <ns0:order>6</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t761"/>
        <ns0:disagreedWith rdf:resource="http://philo.search/thought/t762"/>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t641">
        <ns0:value>Offices and positions must be open to all under conditions of fair equality of opportunity.</ns0:value>
        <ns0:reference>A Theory of Justice, John Rawls, Harvard University Press, 1971</ns0:reference>
        <ns0:order>7</ns0:order>
        <ns0:category>Ethics</ns0:category>
        <ns0:category>Political Philosophy</ns0:category>
      </rdf:Description>
    </ns0:said>

