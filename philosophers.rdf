    <ns0:order>6</ns0:order>
    <ns0:category>Ontology</ns0:category>
  </rdf:Description>

  <rdf:Description rdf:about="http://philo.search/thought/t737">
    <ns0:value>Signification is illocutionary; linguistic conventions are sufficient to endow intentional meaning, independent of the presence of the sender.</ns0:value>
    <ns0:reference>Derrida/Searle, Raoul Moati, Columbia University Press, 2014</ns0:reference>
