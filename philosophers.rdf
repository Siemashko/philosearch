    <ns0:said rdf:resource="http://philo.search/thought/t535"/>
    <ns0:said rdf:resource="http://philo.search/thought/t536"/>
    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t537">
        <ns0:value>When I objectify the other, I deny its selfhood and therefore deny that with respect to which I want to affirm myself.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/sartre-ex/, Author: Christian J. Onof</ns0:reference>
        <ns0:order>4</ns0:order>
        <ns0:category>Epsitemology</ns0:category>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said>
      <rdf:Description rdf:about="http://philo.search/thought/t538">
        <ns0:value>The instability of the dependence upon the other is characteristic of the typically conflictual state of our relations with others.</ns0:value>
        <ns0:reference>The Internet Encyclopedia of Philosophy, iep.utm.edu/sartre-ex/, Author: Christian J. Onof</ns0:reference>
        <ns0:order>5</ns0:order>
        <ns0:category>Ontology</ns0:category>
      </rdf:Description>
    </ns0:said>

    <ns0:said rdf:resource="http://philo.search/thought/t539"/>
    <ns0:said rdf:resource="http://philo.search/thought/t540"/>
    <ns0:said rdf:resource="http://philo.search/thought/t541"/>
